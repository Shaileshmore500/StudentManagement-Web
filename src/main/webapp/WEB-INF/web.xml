<?xml version="1.0" encoding="UTF-8"?>
<element>
	<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
		id="WebApp_ID" version="4.0">

		<display-name>Login</display-name>
		<welcome-file-list>
			<welcome-file>index.jsp</welcome-file>

		</welcome-file-list>


		<servlet>
			<servlet-name>register</servlet-name>
			<servlet-class>com.ytproject.servlet.register_servlet</servlet-class>
		</servlet>

		<servlet-mapping>
			<servlet-name>register</servlet-name>
			<url-pattern>/register_servlet</url-pattern>
		</servlet-mapping>

		<servlet>
			<servlet-name>login</servlet-name>
			<servlet-class>com.ytproject.servlet.login</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>login</servlet-name>
			<url-pattern>/login</url-pattern>
		</servlet-mapping>

		<servlet>
			<servlet-name>logout</servlet-name>
			<servlet-class>com.ytproject.servlet.logout_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>logout</servlet-name>
			<url-pattern>/logout_servlet</url-pattern>
		</servlet-mapping>

		<servlet>
			<servlet-name>edit</servlet-name>
			<servlet-class>com.ytproject.servlet.edit_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>edit</servlet-name>
			<url-pattern>/edit_servlet</url-pattern>
		</servlet-mapping>

		<servlet>
			<servlet-name>add_post</servlet-name>
			<servlet-class>com.ytproject.servlet.add_post</servlet-class>
		</servlet>

		<servlet-mapping>
			<servlet-name>add_post</servlet-name>
			<url-pattern>/add_post</url-pattern>
		</servlet-mapping>
		<servlet>
			<servlet-name>addcategory</servlet-name>
			<servlet-class>com.ytproject.servlet.addcategory</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>addcategory</servlet-name>
			<url-pattern>/addcategory</url-pattern>

		</servlet-mapping>
		<servlet>
			<servlet-name>likeservlet</servlet-name>
			<servlet-class>com.ytproject.servlet.like_servlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>likeservlet</servlet-name>
			<url-pattern>/like_servlet</url-pattern>
		</servlet-mapping>
		<servlet>

			<servlet-name>chatroom</servlet-name>
			<servlet-class>com.ytproject.servlet.ChatRoom</servlet-class>

		</servlet>
		<servlet-mapping>
			<servlet-name>chatroom</servlet-name>
			<url-pattern>/ChatRoom</url-pattern>
		</servlet-mapping>



	</web-app>
</element>